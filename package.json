    "test-ci": "npm run preconfig && ng test --no-watch --no-progress --source-map=false --browsers=ChromeHeadlessCI",
    "lint": "npm run preconfig && prettier --check . && stylelint \"**/*.{css,scss,sass}\" && ng lint",
    "e2e": "npm run preconfig && ng e2e"
  },
  "private": true,
